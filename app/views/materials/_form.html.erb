<%= simple_form_for(@material, :html => {:class=> "form-horizontal"}) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :material_group %>
    <%= f.association :material_type %>
    <%= f.input :quantity %>
    <%= f.association :unit %>
    <%= f.button :submit, :class=> "btn btn-success" %>
  </div>
<% end %>
